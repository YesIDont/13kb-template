{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "outDir": "./dist/",
    "resolveJsonModule": true,
    "sourceMap": false,
    "target": "es5"
  },
  "include": ["src/**/*.ts", "typedefs/*"],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
